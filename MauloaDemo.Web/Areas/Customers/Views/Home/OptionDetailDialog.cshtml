<div class="optiondetail-dialog">

    <form class="form-horizontal" data-role="validator">

        <input type="hidden" data-bind="value: sales.op_seq" name="op_seq" id="hdnOpseq" />

        <div class="form-group">
            <label for="txtItemType" class="col-sm-2 control-label">Type</label>
            <div class="col-sm-2">
                <input type="text" id="txtItemType" name="item_type"
                       maxlength="3" required 
                       placeholder=""
                       class="form-control input-sm toupper tohankaku select-itemtype"
                       style="width:95%;"
                       data-height="300"
                       data-bind="value: sales.item_type, enabled:sales.isNew"
                       data-plantype="O" />
            </div>

            <label for="txtItemCd" class="col-sm-1 control-label">Code</label>
            <div class="col-sm-3">
                <select id="txtItemCd" name="item_cd"
                       maxlength="15" required
                       class="form-control input-sm toupper tohankaku "
                       data-height="400"
                       data-role="combobox"
                       data-value-field="item_cd"
                       data-text-field="item_cd"
                       data-value-primitive="true"
                       data-cascade-from="txtItemType"
                       data-cascade-from-field="item_type"
                       data-bind="value: sales.item_cd, source: itemList, enabled:sales.isNew"
                       data-template="itemCdComboTemplate" 
                       data-header-template="itemCdComboHeaderTemplate"
                ></select>

                <script type="text/template" id="itemCdComboTemplate">
                    <dl>
                    # if (App.User.IsStaff) {#
                        <dt style="width:17%;">${ data.item_cd }</dt>
                        <dd style="width:67%;">${ App.L(data.item_name_jpn, data.item_name) }</dd>
                        <dd style="width:7%;text-align:right;float:right;">$${ data.d_net }</dd>
                        <dd style="width:7%;text-align:right;float:right;">$${ data.gross }</dd>
                    # } else if (App.User.AgentCd === "JTBO"){ #
                        <dt style="width:17%;">${ data.item_cd }</dt>
                        <dd style="width:67%;">${ App.L(data.item_name_jpn, data.item_name) }</dd>
                        <dd style="width:7%;text-align:right;float:right;">$${ data.d_net }</dd>
                        <dd style="width:7%;text-align:right;float:right;">$${ data.gross }</dd>
                    # } else { #
                        <dt style="width:17%;">${ data.item_cd }</dt>
                        <dd style="width:74%;">${ App.L(data.item_name_jpn, data.item_name) }</dd>
                        <dd style="width:7%;text-align:right;float:right;">$${ data.d_net }</dd>
                    # } #
                    </dl>
                </script>

                <script type="text/template" id="itemCdComboHeaderTemplate">
                # if (App.User.IsStaff) {#
                    <div class="clearfix">
                        <dl>
                            <dt style="width:17%;"></dt>
                            <dd style="width:67%;"></dd>
                            <dd style="width:7%;text-align:right;float:right;">Net</dd>
                            <dd style="width:7%;text-align:right;float:right;">Gross</dd>
                        </dl>
                    </div>
                # } else if (App.User.AgentCd === "JTBO"){ #
                    <div class="clearfix">
                        <dl>
                            <dt style="width:17%;"></dt>
                            <dd style="width:67%;"></dd>
                            <dd style="width:7%;text-align:right;float:right;margin-right: 16px;">Net</dd>
                            <dd style="width:7%;text-align:right;float:right;">Gross</dd>
                        </dl>
                    </div>
                # } #
                </script>
            </div>

            @*<label for="" class="col-sm-1 col-sm-offset-1 control-label">Payment</label>
            <div class="col-sm-1">
                <select class="form-control input-sm"
                        style="width:80px;"
                        data-role="dropdownlist"
                        data-value-field="value"
                        data-text-field="text"
                        data-value-primitive="true"
                        data-bind="value: sales.cust_collect, source: sales.PaymentList"></select>
            </div>*@

            <label for="" class="col-sm-1 col-sm-offset-4 control-label">Status</label>
            <div class="col-sm-1">
                <select class="form-control input-sm"
                        style="width:80px;"
                        data-role="dropdownlist"
                        data-value-field="value"
                        data-text-field="text"
                        data-value-primitive="true"
                        data-bind="value: sales.book_status, source: sales.StatusList, disabled: isCanceledOrFinalized"></select>
            </div>

        </div>

        <div class="form-group">
            <label for="txtItemName" class="col-sm-2 control-label">Name</label>
            <div class="col-sm-13">
                <input type="text" readonly
                       class="form-control input-sm k-input k-textbox w100"
                       data-bind="value: item_name_jpn, visible: isJapan" />
                <input type="text" readonly
                       class="form-control input-sm k-input k-textbox w100 " 
                       data-bind="value: item_name, invisible: isJapan" />
            </div>
        </div>

        <div class="form-group">
            <label for="txtQty" class="col-sm-2 control-label">Qty</label>
            <div class="col-sm-1">
                <input type="text" id="txtQty" name="quantity" required
                       data-role="numerictextbox"
                       data-spinners="false"
                       data-decimals="0"
                       data-format="n0"
                       data-max="9999"
                       data-min="1"
                       maxlength="4"
                       class="input-sm numeric"
                       style="width:100%;"
                       data-bind="value: sales.quantity, disabled: isCanceledOrFinalized" />
            </div>

            <label for="txtPrice" class="col-sm-2 control-label" ><span data-bind="invisible: sales.isNew">Price</span></label>
            <div class="col-sm-2 div_txt_price">
                <input type="text" id="txtPrice" name="price"
                       data-role="numerictextbox"
                       data-spinners="false"
                       data-decimals="2"
                       data-format="n2"
                       maxlength="10"
                       class="input-sm numeric"
                       style="width:100%;"
                       data-bind="value: sales.price, enabled: isPriceEditable, invisible: sales.isNew" />
            </div>

            <label for="txtAmount" class="col-sm-2 control-label"><span data-bind="invisible: sales.isNew">Amount</span></label>
            <div class="col-sm-2">
                <input type="text" id="txtAmount"
                       data-role="numerictextbox"
                       data-spinners="false"
                       data-decimals="2"
                       data-format="n2"
                       maxlength="10"
                       readonly
                       class="input-sm numeric"
                       style="width:100%;"
                       data-bind="value: sales.amount, invisible: sales.isNew" />
            </div>
        </div>

        <div class="form-group">
            <label for="txtSubAgent" class="col-sm-2 control-label">Agent</label>
            <div class="col-sm-2">
                <input type="text" id="txtSubAgent" name="sub_agent_cd" required maxlength="6"
                       class="form-control input-sm select-subagent toupper tohankaku"
                       style="width:80px"
                       data-bind="value: sales.sub_agent_cd, disabled: isCanceledOrFinalized" />
            </div>

            <label for="txtInvAgent" class="col-sm-1 control-label">Invoice</label>
            <div class="col-sm-2" style="padding-left: 4px;">
                <input type="text" id="txtInvAgent" name="inv_agent" required maxlength="6"
                       class="form-control input-sm select-invagent toupper tohankaku"
                       style="width:80px;"
                       data-bind="value: sales.inv_agent, disabled: isCanceledOrFinalized" />
            </div>

            <label for="txtPriceChangeReason"
                   class="col-sm-2 control-label" style="display:none;"
                   data-bind="visible: sales.price_changed">Reason</label>
            <div class="col-sm-3">
                <input type="text" id="txtPriceChangeReason"
                       maxlength="200"
                       class="form-control input-sm k-input k-textbox"
                       style="width:100%;"
                       data-bind="value: sales.price_change_reason, visible: sales.price_changed, enabled: isPriceChangeReasonEditable" />
            </div>

            <div class="col-sm-4 created_by pull-right">
                <table>
                    <tr>
                        <td>Created: </td>
                        <td>
                            <span data-bind="text: sales.create_by"></span>
                            (<span data-bind="text: sales.create_date_s"></span>)
                        </td>
                    </tr>
                    <tr>
                        <td>Updated: </td>
                        <td>
                            <span data-bind="text: sales.last_person"></span>
                            (<span data-bind="text: sales.update_date_s"></span>)
                        </td>
                    </tr>
                </table>
            </div>
        </div>

        <div class="form-group">
            <div class="col-sm-8 pull-right">
                <label class="col-sm-3 control-label">Staff</label>
                <div class="col-sm-4">
                    <input type="text" name="Staff" required maxlength="50"
                           class="form-control input-sm k-input k-textbox toupper tohankaku"
                           style="width:100%;"
                           data-bind="value: sales.staff, disabled: isCanceledOrFinalized" />
                </div>

                <label class="col-sm-3 control-label" data-bind="visible: isBranchStaffVisible">店舗担当者</label>
                <div class="col-sm-4">
                    <input type="text" name="Branch Staff" maxlength="50"
                           class="branch_staff form-control input-sm k-input k-textbox toupper tohankaku"
                           style="width:100%;"
                           data-bind="value: sales.branch_staff, visible: isBranchStaffVisible, disabled: isCanceledOrFinalized" />
                </div>
            </div>
        </div>


        @* Info Section *@
        <section class="section-info clearfix">
            <ul class="nav nav-tabs nav-tabs-info">
                <li data-bind="visible: visibleDLV" class="active"><a href="#panDLV" data-toggle="tab">Delivery</a></li>
                <li data-bind="visible: visibleMKS" class="active"><a href="#panMKS" data-toggle="tab">Make Up/Aesthe</a></li>
                <li data-bind="visible: visibleSHT" class="active"><a href="#panSHT" data-toggle="tab">Photo Shooting</a></li>
                <li data-bind="visible: visibleRCP" class="active"><a href="#panRCP" data-toggle="tab">Reception</a></li>
                <li data-bind="visible: visibleTRN" class="active"><a href="#panTRN" data-toggle="tab">Transportation</a></li>
                <li data-bind="visible: visibleOthers" class="active"><a href="#panOthers" data-toggle="tab">Others</a></li>
            </ul>

            <div class="tab-content">
                @* DLV *@
                <div class="tab-pane active" id="panDLV"
                     data-bind="visible: visibleDLV">
                    <div class="form-group">
                        <label for="" class="col-sm-1 control-label">Date</label>
                        <div class="col-sm-3">
                            <input type="text" class="form-control input-sm todate"
                                   maxlength="10"
                                   style="width:100px;"
                                   data-role="datepicker"
                                   data-bind="value: sales.DeliveryInfo.delivery_date, disabled: isCanceledOrFinalized" />
                        </div>

                        <label for="" class="col-sm-1 control-label">Time</label>
                        <div class="col-sm-2">
                            <input type="text" class="form-control input-sm k-input k-textbox totime"
                                   maxlength="5"
                                   placeholder="hh:mm"
                                   data-bind="value: sales.DeliveryInfo.delivery_time_s, disabled: isCanceledOrFinalized" />
                        </div>

                        <label for="" class="col-sm-1 control-label">Place</label>
                        <div class="col-sm-3">
                            <input type="text" class="form-control input-sm k-input k-textbox"
                                   maxlength="50"
                                   data-bind="value: sales.DeliveryInfo.delivery_place, disabled: isCanceledOrFinalized" />
                        </div>
                    </div>

