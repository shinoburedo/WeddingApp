        <section class="section-arrangement" data-bind="visible: isArrangementVisible">
            <div class="buttons-arrangement clearfix">
                <div class="pull-left" style="margin-left:60px;">
                    <button id="btnAddArrangement" type="button"
                            data-bind="click:onAddArrangement, disabled: isCanceledOrFinalized, visible: isSaveButtonVisible"
                            class="btn btn-primary">
                        <span class="glyphicon glyphicon-plus"></span> Add Vendor
                    </button>
                </div>

                <div class="pull-right" style="margin-right: 100px;">
                    <button type="button" class="btn btn-primary"
                            data-contenturl='@Url.Action("PreviewVendorOrderSheet")'
                            data-bind="events: {click: onOrderSheetClick}, disabled: isCanceledOrFinalized">
                        Order Sheet
                    </button>
                </div>
            </div>

            <div id="grdArrangement"
                 data-role="grid"
                 data-auto-bind="true"
                 data-bind="source: Arrangements"
                 data-editable="{mode: 'incell', createAt:'bottom'}"
                 data-sortable="false"
                 data-resizable="true"
                 data-selectable="true"
                 data-scrollable="true"
                 data-groupable="false"
                 data-navigatable="false"
                 data-height="180"
                 data-columns="[ {field: 'vendor_cd', title: 'Vendor', width: 70, editor: vendorEditor  }, {field: 'cfmd', title: 'Cfmd', width: 50, template: '<input type=\'checkbox\' data-bind=\'checked: cfmd\' \/>', attributes: ' class=\'centered\'' }, {field: 'cfmd_by', title: 'By', width: 60 }, {field: 'cfmd_date', title: 'Cfmd Date', width: 100, format:'{0:MM/dd/yyyy}'}, {field: 'cxl', title: 'CXL', width: 50, template: '<input type=\'checkbox\' data-bind=\'checked: cxl\' \/>', attributes: ' class=\'centered\'' }, {field: 'cxl_vend_by', title: 'By', width: 60, editor: personEditor }, {field: 'cxl_date', title: 'Cxl Date', width: 100, format:'{0:MM/dd/yyyy}'}, {field: 'note', title: 'Note', editor: noteEditor}, {field: 'quantity', title: 'Qty', width: 40, hidden:true, attributes: ' class=\'col_numeric\'', headerAttributes: ' class=\'col_numeric\'' }, {field: 'cost', title: 'Cost', width: 60, hidden:true, format: '{0:0.00}', attributes: ' class=\'col_numeric\'', headerAttributes: ' class=\' col_numeric\'' } ]">
            </div>
        </section>

        <div class="bottom-buttons clearfix">
            <input type="button" value=" SAVE "
                   class="btn btn-danger" style="width:100px;display:none;"
                   data-bind="click: onOkClick, disabled: isCanceledOrFinalized, visible: isSaveButtonVisible" />
            <input type="button" value="Close"
                   class="btn btn-default" style="width:100px; margin-left: 100px;"
                   data-bind="click: closeDialog" />
        </div>
    </form>
</div>

<script>

    function vendorEditor(container, options) {
        var item_cd = App.optionDetailViewModel.get("sales.item_cd");
        var data = { item_cd: item_cd };

        var input = $("<input/>");
        input.attr("name", options.field);
        input.appendTo(container);
        input.kendoComboBox({
            valuePrimitive: true,
            dataValueField: "vendor_cd",
            dataTextField: "vendor_cd",
            dataSource: App.data.get("vendorList")(data),
            template: "<dl><dt>${ data.vendor_cd }</dt><dd>${ data.vendor_name }</dd></dl>",
        });
        input.data("kendoComboBox").list.width("400");
    }

    function noteEditor(container, options) {
        var input = $('<textarea data-bind="value:' + options.field + '" />');
        input.appendTo(container);
    }

    

    function personEditor(container, options) {
        var input = $("<input/>");
        input.attr("name", options.field);
        input.appendTo(container);
        input.kendoComboBox({
            valuePrimitive: true,
            dataValueField: "login_id",
            dataTextField: "login_id",
            dataSource: new kendo.data.DataSource({
                transport: {
                    read: {
                        url: App.getApiPath("LoginUsers/GetList"),
                        dataType: "json"
                    }
                }
            }),
            template: "<dl><dt style='width:120px;'>${ data.login_id }</dt><dd>${ (data.e_last_name || '') + ' ' + (data.e_first_name || '') }</dd></dl>",
        });
        input.data("kendoComboBox").list.width("460");
    }
</script>



