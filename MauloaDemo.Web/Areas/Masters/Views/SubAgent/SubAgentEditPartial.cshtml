<style scoped>
    #editDiv {
        width: 100% !important;
    }

</style>

<div id="editWindow"
     data-role="window"
     data-visible="false"
     data-modal="true"
     data-min-width="800"
     data-width="800"
     data-min-height="500"
     data-height="650"
     style="display:none;">

    <div id="editDiv">
        <form class="form-horizontal" data-role="validator">
            <div class="form-group">
                <label for="child_cd" class="col-sm-4 control-label required">Code</label>
                <div class="col-sm-2">
                    <input name="child_cd" type="text" data-bind="value: model.child_cd, enabled: isCodeEditable"
                           maxlength="6" required
                           class="form-control input-sm k-input k-textbox toupper tohankaku" />
                </div>
            </div>

            <div class="form-group">
                <label for="parent_cd" class="col-sm-4 control-label">Parent Cd</label>
                <div class="col-sm-3">
                    <input type="text" name="parent_cd"
                           maxlength="6"
                           placeholder=""
                           class="form-control input-sm toupper tohankaku select-agent"
                           style="width:95%;"
                           data-height="300"
                           data-bind="value: model.parent_cd" />
                </div>
                <label for="inv_agent" class="col-sm-3 control-label">Inv Agent</label>
                <div class="col-sm-3">
                    <input type="text" name="inv_agent"
                           maxlength="6"
                           placeholder=""
                           class="form-control input-sm toupper tohankaku select-agent"
                           style="width:95%;"
                           data-height="300"
                           data-bind="value: model.inv_agent" />
                </div>
            </div>

            <div class="form-group">
                <label for="invoice_type" class="col-sm-4 control-label">Invoice Type</label>
                <div class="col-sm-2">
                    <input name="invoice_type" type="text" data-bind="value: model.invoice_type"
                           maxlength="1"
                           class="form-control input-sm k-input k-textbox toupper tohankaku" />
                </div>
                <label for="indep_flg" class="col-sm-3 control-label">Indep Flg</label>
                <div class="col-sm-1">
                    <input name="indep_flg" type="checkbox" data-bind="checked: model.indep_flg" />
                </div>
            </div>

            <div class="form-group">
                <label for="contact_name" class="col-sm-4 control-label">Contact Name</label>
                <div class="col-sm-10">
                    <input name="contact_name" type="text" data-bind="value: model.contact_name" maxlength="50"
                           class="form-control input-sm k-input k-textbox " />
                </div>
            </div>

            <div class="form-group">
                <label for="company_name" class="col-sm-4 control-label">Company Name Jpn</label>
                <div class="col-sm-10">
                    <input name="company_name" type="text" data-bind="value: model.company_name" maxlength="100"
                           class="form-control input-sm k-input k-textbox " />
                </div>
            </div>

            <div class="form-group">
                <label for="company_name_eng" class="col-sm-4 control-label">Eng</label>
                <div class="col-sm-10">
                    <input name="company_name_eng" type="text" data-bind="value: model.company_name_eng" maxlength="100"
                           class="form-control input-sm k-input k-textbox " />
                </div>
            </div>

            <div class="form-group">
                <label for="address1" class="col-sm-4 control-label">Address 1</label>
                <div class="col-sm-10">
                    <input name="address1" type="text" data-bind="value: model.address1" maxlength="100"
                           class="form-control input-sm k-input k-textbox " />
                </div>
            </div>

            <div class="form-group">
                <label for="address2" class="col-sm-4 control-label">2</label>
                <div class="col-sm-10">
                    <input name="address2" type="text" data-bind="value: model.address2" maxlength="100"
                           class="form-control input-sm k-input k-textbox " />
                </div>
            </div>

            <div class="form-group">
                <label for="address3" class="col-sm-4 control-label">3</label>
                <div class="col-sm-10">
                    <input name="address3" type="text" data-bind="value: model.address3" maxlength="100"
                           class="form-control input-sm k-input k-textbox " />
                </div>
            </div>

            <div class="form-group">
                <label for="tel1" class="col-sm-4 control-label">Phone1</label>
                <div class="col-sm-3">
                    <input name="tel1" type="text" data-bind="value: model.tel1" maxlength="20"
                           class="form-control input-sm k-input k-textbox tohankaku " />
                </div>
                <label for="tel2" class="col-sm-1 control-label">2</label>
                <div class="col-sm-3">
                    <input name="tel2" type="text" data-bind="value: model.tel2" maxlength="20"
                           class="form-control input-sm k-input k-textbox tohankaku " />
                </div>
            </div>
            <div class="form-group">
                <label for="fax" class="col-sm-4 control-label">Fax</label>
                <div class="col-sm-3">
                    <input name="fax" type="text" data-bind="value: model.fax" maxlength="20"
                            class="form-control input-sm k-input k-textbox tohankaku " />
                </div>
            </div>

            <div class="form-group">
                <label for="email" class="col-sm-4 control-label">Email</label>
                <div class="col-sm-10">
                    <input name="email" type="text" data-bind="value: model.email" maxlength="50"
                           class="form-control input-sm k-input k-textbox " />
                </div>
            </div>

            <div class="form-group">
                <label for="agent_area_cd" class="col-sm-4 control-label">Area</label>
                <div class="col-sm-4">
                    <input name="agent_area_cd" type="text" data-bind="value: model.agent_area_cd" maxlength="5"
                           class="form-control input-sm k-input k-textbox " />
                </div>
            </div>

            @*<div class="form-group">
                <label for="cos_kbn" class="col-sm-4 control-label ">Cos Kbn</label>
                <div class="col-sm-2">
                    <input name="cos_kbn" type="text" data-bind="value: model.cos_kbn"
                           maxlength="1"
                           class="form-control input-sm k-input k-textbox toupper tohankaku" />
                </div>
                <label for="logi_kbn" class="col-sm-2 control-label ">Logi Kbn</label>
                <div class="col-sm-2">
                    <input name="logi_kbn" type="text" data-bind="value: model.logi_kbn"
                           maxlength="1"
                           class="form-control input-sm k-input k-textbox toupper tohankaku" />
                </div>
            </div>

            <div class="form-group">
                <label for="stock_agent" class="col-sm-4 control-label ">Stock Agent</label>
                <div class="col-sm-2">
                    <input name="stock_agent" type="text" data-bind="value: model.stock_agent"
                           maxlength="6"
                           class="form-control input-sm k-input k-textbox toupper tohankaku" />
                </div>
            </div>*@

            <div class="form-group">
                <label for="discon_date" class="col-sm-4 control-label">Discon Date</label>
                <div class="col-sm-3">
                    <input name="discon_date" type="text" 
                           style="width:100px;" maxlength="10"
                           data-role="datepicker"
                           data-bind="value: model.discon_date" 
                           class="form-control input-sm tohankaku " />
                </div>
            </div>

            @* Saveボタン、Closeボタン *@
            <div class="buttons">
                <input id="btnEditSave" type="button" value="Save" data-bind="click:onSave" class="btn btn-primary btn-save" />
                <input id="btnEditClose" type="button" value="Close" data-bind="click: onClose" class="btn btn-default btn-close" />
            </div>
        </form>
    </div>

</div> @* End of editWindow *@

